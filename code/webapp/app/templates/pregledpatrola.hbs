<table class="table table-hover tabelica">
  <thead>
    <tr>
      <th>Vrijeme prijave</th>
      <th>Lokacija</th>
      <th>Vrsta kontrole</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |patrola|}}  
    <tr>
      <td>{{patrola.vrijeme_prijave}}</td>
      <td>{{patrola.lokacija.naziv}}</td>
      <td>{{patrola.vrsta_kontrole}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

{{outlet}}
